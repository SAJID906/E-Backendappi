{
  "version": 2,
  "builds": [
      { "src": "Backend/index.js", "use": "@vercel/node" },
      { "src": "frontend/public/index.html", "use": "@vercel/static" }
  ],
   "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }] ,
   "rewrites": [ { "source": "/(.*)", "dest": "/index.html" } ] 
}
